package javaDemo;

public class TestFor {
	public static void main(String[] args){
		/*
		for(int i = 1;i<=9;i++){
			for(int j = 1;j<=i;j++){
				System.out.print(i+"*"+j+"="+i*j+"\t");
			}
			System.out.println();
		}
		*/
		outer:for(int i=1;i<10;i++){
			for(int j = 2;j<=i/2;j++){
